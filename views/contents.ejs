<%- include("partials/header") %>

<% content.forEach(function(contents){ %>
    <div>
        <h2><%= contents.title %></h2>
        <img src="<%= contents.image %>" alt="image">
        <article><%= contents.description %></article>
        <a href="/contents/<%= contents._id %>">More</a>
    </div>
<% }) %>
<%- include("partials/footer") %>